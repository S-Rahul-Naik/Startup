{
  "name": "edutech-website",
  "version": "1.0.0",
  "description": "Complete full-stack website for Edu Tech - Academic Project Management Platform",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
